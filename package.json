{
  "name": "aos-server-utils-2",
  "version": "1.0.0",
  "description": "Apconic server utilities. Contains Custom errors, keycloak auth wrapper etc",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prettier-check": "prettier  --check lib/**/*.ts",
    "prettier-format": "prettier --config .prettierrc \"lib/**/*.ts\" --write",
    "lint": "eslint --ext .ts lib",
    "prepublish": "npm run lint && npm run prettier-check && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apconic/aos-server-utils.git"
  },
  "keywords": [
    "Apconic"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apconic/aos-server-utils/issues"
  },
  "homepage": "https://github.com/apconic/aos-server-utils#readme",
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/node": "20.4.9",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.1",
    "rimraf": "5.0.1",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "express": "4.18.2",
    "graphql-request": "6.1.0",
    "inversify": "6.0.1",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13"
  }
}
